	"Marbles": {
		"edgelessdb": {
			"Package": "edgelessdb",
			"Parameters": {
				"Env": {
					"EROCKSDB_MASTERKEY": "{{ hex .Secrets.edbMasterKey.Private }}",
					"EDB_ROOT_CERT": "{{ pem .Secrets.edbRootCert.Cert }}",
					"EDB_ROOT_KEY": "{{ pem .Secrets.edbRootCert.Private }}",
					"EDG_EDB_MANIFEST_FILE": "/data/manifest.json",
					"EDG_EDB_CERT_DNS": "mysql.bookwarehouse"
				},
				"Argv": [
					"./edb",
					"-marble"
				],
				"Files": {
					"/data/manifest.json": {
						"Encoding": "base64",
						"NoTemplates": false,
						"Data": ""
					}
				}
			}
		},
		"bookbuyer": {
			"Package": "bookbuyer",
			"Parameters": {
				"Argv": [
					"/bookbuyer",
					"--port",
					"14001"
				],
				"Env": {
					"BOOKSTORE_NAMESPACE": "bookstore",
					"BOOKSTORE_SVC": "bookstore"
				}
			},
			"TLS": [
				"bookbuyer-tls"
			]
		},
		"bookthief": {
			"Package": "bookthief",
			"Parameters": {
				"Argv": [
					"/bookthief",
					"--port",
					"14001"
				],
				"Env": {
					"BOOKSTORE_NAMESPACE": "bookstore",
					"BOOKSTORE_SVC": "bookstore",
					"BOOKTHIEF_EXPECTED_RESPONSE_CODE": "503"
				}
			},
			"TLS": [
				"bookthief-tls"
			]
		},
		"bookstore": {
			"Package": "bookstore",
			"Parameters": {
				"Argv": [
					"/bookstore",
					"--port",
					"14001"
				],
				"Env": {
					"BOOKWAREHOUSE_NAMESPACE": "bookwarehouse",
					"IDENTITY": "bookstore-v1"
				}
			},
			"TLS": [
				"bookstore-tls"
			]
		},
		"bookstore-v2": {
			"Package": "bookstore",
			"Parameters": {
				"Argv": [
					"/bookstore",
					"--port",
					"14001"
				],
				"Env": {
					"BOOKWAREHOUSE_NAMESPACE": "bookwarehouse",
					"IDENTITY": "bookstore-v2"
				}
			},
			"TLS": [
				"bookstoreV2-tls"
			]
		},
		"bookwarehouse": {
			"Package": "bookwarehouse",
			"Parameters": {
				"Argv": [
					"/bookwarehouse",
					"--port",
					"14001"
				],
				"Env": {
					"SQL_CERT": "{{ pem .Secrets.sqlCert.Cert }}",
					"SQL_KEY": "{{ pem .Secrets.sqlCert.Private }}"
				}
			},
			"TLS": [
				"bookwarehouse-tls"
			]
		}
	},
	"Secrets": {
		"sqlCert": {
			"Type": "cert-ecdsa",
			"Size": 256,
			"Shared": false,
			"Cert": {
				"Subject": {
					"CommonName": "bookwarehouse"
				}
			}
		},
		"edbMasterKey": {
			"Size": 128,
			"Shared": true,
			"Type": "symmetric-key"
		},
		"edbRootCert": {
			"Size": 256,
			"Type": "cert-ecdsa",
			"Shared": true,
			"Cert": {
				"IsCA": true,
				"Subject": {
					"Organization": [
						"My EdgelessDB root"
					]
				},
				"DNSNames": [
					"mysql.bookwarehouse"
				]
			}
		}
	},
	"TLS": {
		"bookbuyer-tls": {
			"Outgoing": [
				{
					"Port": "14001",
					"Addr": "bookstore.bookstore.svc.cluster.local"
				},
